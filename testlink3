<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>GitHub Pages Test</title>
</head>
<body>
    <h2>GitHub Pages is Working âœ…</h2>
    <p id="output">Fetching data...</p>

    <script>
        // Get URL Parameters
        let urlParams = new URLSearchParams(window.location.search);
        let payload = urlParams.get("payload");

        // Check if payload exists and update content
        if (payload) {
            document.getElementById("output").innerText = `Payload: ${payload}`;
        } else {
            document.getElementById("output").innerText = "No payload found.";
        }
    </script>
</body>
</html>
